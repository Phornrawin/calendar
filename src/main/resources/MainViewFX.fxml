<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="664.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainViewController">
   <children>
      <Label layoutX="141.0" layoutY="14.0" text="Appointment Schedule">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextArea fx:id="showEventTextArea" layoutX="61.0" layoutY="179.0" prefHeight="367.0" prefWidth="484.0" />
      <Label fx:id="selectDayTextView" layoutX="76.0" layoutY="120.0" text="Select day: ">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnAddEvent" layoutX="141.0" layoutY="586.0" mnemonicParsing="false" onAction="#onClickNewEvent" text="Add Event" />
      <Button fx:id="btnEditAndDelete" layoutX="342.0" layoutY="586.0" mnemonicParsing="false" onAction="#onClickEditAndDelete" text="Edit/Delete" />
      <DatePicker fx:id="datepickerSelectDay" layoutX="180.0" layoutY="118.0" prefHeight="31.0" prefWidth="311.0" />
   </children>
</AnchorPane>
